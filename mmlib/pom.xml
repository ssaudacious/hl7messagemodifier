<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>gov.nist.healthcare.mu</groupId>
	<artifactId>mmlib</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Message Modifier Library</name>
	<description>Provides the backend support for the Message Modifier Project</description>
	<dependencies>
		<dependency>
			<groupId>org.immunizationsoftware.dqa</groupId>
			<artifactId>tester</artifactId>
			<version>1.36</version>
		</dependency>
		<dependency>
			<artifactId>core-hl7-v2-message</artifactId>
			<groupId>gov.nist.healthcare.core</groupId>
			<version>1.0.49</version>
		</dependency>
		<!-- MySQL database driver -->
		<dependency>
			<groupId>mysql</groupId>
			<artifactId>mysql-connector-java</artifactId>
			<version>5.1.26</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-context</artifactId>
			<version>3.2.1.RELEASE</version>
			<exclusions>
				<!-- Exclude Commons Logging in favor of SLF4j -->
				<exclusion>
					<groupId>commons-logging</groupId>
					<artifactId>commons-logging</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-beans</artifactId>
			<version>4.0.3.RELEASE</version>
		</dependency>


		<!-- Hibernate framework -->
		<dependency>
			<groupId>org.hibernate</groupId>
			<artifactId>hibernate-core</artifactId>
			<version>4.2.4.Final</version>
		</dependency>

		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-orm</artifactId>
			<version>4.0.3.RELEASE</version>
		</dependency>

		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-dbcp2</artifactId>
			<version>2.1</version>
		</dependency>

		<dependency>
			<groupId>org.javassist</groupId>
			<artifactId>javassist</artifactId>
			<version>3.18.0-GA</version>
		</dependency>

		<dependency>
			<groupId>org.apache.tomcat</groupId>
			<artifactId>tomcat-dbcp</artifactId>
			<version>7.0.41</version>
			<scope>provided</scope>
		</dependency>

		<dependency>
			<artifactId>core-hl7-v2-parser</artifactId>
			<groupId>gov.nist.healthcare.core</groupId>
			<version>1.0.49</version>
		</dependency>
		<dependency>
			<artifactId>core-hl7-v2-message</artifactId>
			<groupId>gov.nist.healthcare.core</groupId>
			<version>1.0.49</version>
		</dependency>
	</dependencies>
</project>